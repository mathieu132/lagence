                                <li><a href="{{ path("selection") }}">Sélection</a></li>
                                <li><a href="">utilisateur</a></li>
                            </ul>
                        </li>
                       

                        {% if is_granted("IS_AUTHENTICATED_FULLY") %}
                          <li>
                            <a href="{{ path("app_logout") }}" class="lien">Déconnexion</a>
                        </li>
                        {% else %}
                        <li>
                            <a href="{{ path("app_login") }}" class="lien">Connexion</a>
                        </li>
                         <li>
                            <a href="{{ path("app_register") }}" class="lien">Inscription</a>
                        </li>
                       {% endif %}
                        <li>
                            <a href="#" class="lien1">06 84 84 18 90</a>
